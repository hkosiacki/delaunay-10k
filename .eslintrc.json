{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-console": 0,
    "valid-jsdoc": 2,
    "curly": 2,
    "default-case": 2,
    "dot-location": [2, "property"],
    "eqeqeq": 2,
    "guard-for-in": 1,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-loop-func": 2,
    "no-native-reassign": 2,
    "no-new-wrappers": 2,
    "no-useless-concat": 1,
    "no-use-before-define": 2,
    "no-var": 2
  }
}
